# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Environment
REACT_APP_ENV=development

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Error Tracking (optional)
REACT_APP_SENTRY_DSN=your-sentry-dsn-here

# Feature Flags (optional)
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=false
REACT_APP_ENABLE_OFFLINE_MODE=true

# App Configuration
REACT_APP_NAME=NutriTrack
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=AI-Powered Nutrition Tracker

# External Services
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_NUTRITIONIX_APP_ID=your-nutritionix-app-id

# PWA Configuration
REACT_APP_PWA_ENABLED=true
REACT_APP_OFFLINE_CACHE_VERSION=1

# Development Tools
REACT_APP_DEBUG_MODE=false
REACT_APP_MOCK_API=false
